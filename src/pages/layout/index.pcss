@tailwind base;
@tailwind components;
@tailwind utilities;

@font-face {
	font-family: 'PermanentMarker';
	font-weight: 600;
	src: url('/font/PermanentMarker-Regular.ttf');
}
:root {
	@apply dark:text-white;
}

kbd {
	@apply border-2 border-b-4 border-gray-300 bg-gray-100 py-1 px-2 rounded-md dark:bg-gray-500 dark:border-gray-600;
}